{
  "name": "MilSearches",
  "description": "MilSearches: write down on the finder bar 'mil' and then 'tab'. After that write down whatever you want to look for in milanuncios!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "mil"
  },
  "action": {
    "default_icon": {
      "16": "/images/logoMA.png",
      "32": "/images/logoMA.png",
      "48": "/images/logoMA.png",
      "128": "/images/logoMA.png"
    }
  },
  "icons": {
    "16": "/images/logoMA.png",
    "32": "/images/logoMA.png",
    "48": "/images/logoMA.png",
    "128": "/images/logoMA.png"
  }
}
